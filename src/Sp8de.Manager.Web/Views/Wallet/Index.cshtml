                        <div style="text-align: center; margin-bottom: 20px">
                            <span class="payment-error" id="ADAError"></span>
                        </div>

                        <input type="submit" value="RETRY" id="ADARetry" style="display: none;" class="retry-button" data-currency="ADA" data-parsley-group="block-3">
                    </div>
                    <div class="qr-code-block hidden-block">
                        <div id="ADAWalletCode" class="qr-wrap qr-image"></div>
                        <div class="qr-wallet-info">
